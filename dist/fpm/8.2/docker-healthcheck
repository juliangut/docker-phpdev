set -e

PING=$(env -i REQUEST_METHOD=GET SCRIPT_NAME=/ping SCRIPT_FILENAME=/ping cgi-fcgi -bind -connect 127.0.0.1:9000 2> /dev/null)
if [[ "$(echo "$PING" | tail -n 1)" == "pong" ]]; then
  exit 0;
fi;

exit 1

