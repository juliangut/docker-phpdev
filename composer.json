{
  "name": "juliangut/docker-phpdev",
  "description": "PHP development Docker image",
  "keywords": [
    "Docker",
    "development",
    "image"
  ],
  "type": "project",
  "homepage": "https://github.com/juliangut/docker-phpdev",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Julián Gutiérrez",
      "email": "juliangut@gmail.com",
      "homepage": "http://juliangut.com",
      "role": "Developer"
    }
  ],
  "support": {
    "source": "https://github.com/juliangut/docker-phpdev",
    "issues": "https://github.com/juliangut/docker-phpdev/issues"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^7.0",
    "ext-json": "*",
    "symfony/console": "^3.0",
    "twig/twig": "^2.0"
  },
  "require-dev": {
  },
  "suggest": {
  },
  "autoload": {
    "psr-4": {
      "Jgut\\Docker\\PhpDev\\": "src/"
    }
  },
  "autoload-dev": {
  },
  "bin": [
    "bin/phpdev"
  ],
  "config": {
  },
  "scripts": {
    "build": "phpdev"
  },
  "extra": {
    "versions": {
      "cli": {
        "8.0": {
          "php_version": "8.0",
          "php_image": "php:8.0-rc-cli-alpine",
          "root_allowed": false,
          "use_xdebug": false
        },
        "7.4": {
          "php_version": "7.4",
          "php_image": "php:7.4-alpine",
          "root_allowed": false
        },
        "7.3": {
          "php_version": "7.3",
          "php_image": "php:7.3-alpine",
          "root_allowed": false
        },
        "7.2": {
          "php_version": "7.2",
          "php_image": "php:7.2-alpine",
          "root_allowed": false
        }
      },
      "fpm": {
        "8.0": {
          "php_version": "8.0",
          "php_image": "php:8.0-rc-cli-fpm-alpine",
          "use_xdebug": false
        },
        "7.4": {
          "php_version": "7.4",
          "php_image": "php:7.4-fpm-alpine"
        },
        "7.3": {
          "php_version": "7.3",
          "php_image": "php:7.3-fpm-alpine"
        },
        "7.2": {
          "php_version": "7.2",
          "php_image": "php:7.2-fpm-alpine"
        }
      },
      "jenkins": {
        "8.0": {
          "php_version": "8.0",
          "php_image": "php:8.0-rc-cli-alpine",
          "use_xdebug": false
        },
        "7.4": {
          "php_version": "7.4",
          "php_image": "php:7.4-alpine"
        },
        "7.3": {
          "php_version": "7.3",
          "php_image": "php:7.3-alpine"
        },
        "7.2": {
          "php_version": "7.2",
          "php_image": "php:7.2-alpine"
        }
      }
    }
  }
}
